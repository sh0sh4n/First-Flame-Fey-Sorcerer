   <text:p text:style-name="Preformatted_20_Text"><text:tab/><text:tab/><text:tab/>&lt;grant type=&quot;Spell&quot; id=&quot;ID_PHB_SPELL_CALM_EMOTIONS&quot; level=&quot;3&quot; spellcasting=&quot;Sorcerer&quot; /&gt;&#x0d;</text:p>
   <text:p text:style-name="Preformatted_20_Text"><text:tab/><text:tab/><text:tab/>&lt;grant type=&quot;Spell&quot; id=&quot;ID_PHB_SPELL_HYPNOTIC_PATTERN&quot; level=&quot;5&quot; spellcasting=&quot;Sorcerer&quot; /&gt;&#x0d;</text:p>
   <text:p text:style-name="Preformatted_20_Text"><text:tab/><text:tab/><text:tab/>&lt;grant type=&quot;Spell&quot; id=&quot;ID_PHB_SPELL_GREATER_INVISIBILITY&quot; level=&quot;7&quot; spellcasting=&quot;Sorcerer&quot; /&gt;&#x0d;</text:p>
